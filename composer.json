{
  "name": "christianblos/codedocs",
  "description": "Put your project documentation directly in your code!",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Blos",
      "email": "christian.blos@gmx.de"
    }
  ],
  "require": {
    "doctrine/annotations": "~1.2",
    "symfony/yaml": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8"
  },
  "bin": [
    "codedocs"
  ],
  "autoload": {
    "psr-0": {
      "CodeDocs\\": "classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CodeDocs\\Test\\": "tests/unit"
    },
    "classmap": [
      "examples"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}
