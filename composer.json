{
  "name": "codedocs/codedocs",
  "description": "Let your project documentation automatically update when your code changes!",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Blos",
      "email": "christian.blos@gmx.de"
    }
  ],
  "require": {
    "php": "^7.2|^8.0",
    "nikic/php-parser": "^4.2",
    "doctrine/annotations": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "christianblos/markdown2html": "^0.2"
  },
  "bin": [
    "codedocs"
  ],
  "autoload": {
    "psr-4": {
      "CodeDocs\\": [
        "src/",
        "annotations/"
      ],
      "CodeDocs\\Func\\": "functions"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.x-dev"
    }
  }
}
